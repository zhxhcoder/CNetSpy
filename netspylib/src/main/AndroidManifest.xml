<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.creditease.netspy">
    <application>
        <provider
                android:name=".internal.data.NetSpyContentProvider"
                android:authorities="${applicationId}.netspy.provider"
                android:exported="false"/>
        <activity
                android:name=".internal.ui.NetSpyHomeActivity"
                android:label="@string/netspy_name"
                android:launchMode="singleTask"
                android:taskAffinity="com.creditease.netspy.task"
                android:theme="@style/NetSpy.Theme"/>
        <activity android:name=".internal.ui.TransactionActivity"
                  android:theme="@style/NetSpy.Theme"
                  android:parentActivityName=".internal.ui.NetSpyHomeActivity"/>
        <service
                android:name=".internal.support.ClearTransactionsService"
                android:exported="false"/>
    </application>
</manifest>